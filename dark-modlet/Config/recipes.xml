<configs>
	<append xpath="/recipes" >
		<!-- wall Oven -->
		<recipe name="Wall Oven" count="1" >
			<ingredient name="resourceForgedIron" count="10" />
			<ingredient name="resourceMechanicalParts" count="2" />
			<ingredient name="resourceElectricParts" count="4" />
			<ingredient name="resourceMetalPipe" count="2" />
			<ingredient name="glassIndustrialCTRPlate" count="2" />
		</recipe>
		<!-- Jail Door -->
		<recipe name="Jail Prision" count="1" craft_area="workbench">
			<ingredient name="resourceForgedIron" count="25"/>
			<ingredient name="resourceSpring" count="7"/>
			<ingredient name="resourceMechanicalParts" count="17"/>
		</recipe>
		<!-- Garage door -->
		<recipe name="Folding Garage Door" count="1" craft_area="workbench">
			<ingredient name="resourceWood" count="60"/>
     		<ingredient name="resourceScrapIron" count="25"/>
			<ingredient name="resourceSpring" count="7"/>
			<ingredient name="resourceMechanicalParts" count="16"/>
		</recipe>
		<!-- Jail Bars -->
		<recipe name="Jails Bars" count="1" craft_area="workbench">
     		<ingredient name="resourceForgedIron" count="20"/>
			<ingredient name="resourceSpring" count="5"/>
		</recipe>
		<!-- Concrete Barrier Door -->
		<recipe name="Barrier Concrete" count="1" craft_area="cementMixer">
			<ingredient name="resourceConcreteMix" count="50"/>
		</recipe>
	</append>
</configs>